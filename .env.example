# Ambiente
NODE_ENV=test

# Servidor
PORT=3101  # Porta diferente para evitar conflitos

# Banco de Dados (PostgreSQL)
DATABASE_URL=postgresql://docker:docker@localhost:5432/gympass_tests

# Redis (Cache)
REDIS_HOST=127.0.0.1
REDIS_PORT=6379

# JWT (Autenticação)
JWT_SECRET=test-jwt-secret-for-testing-only-min-32-chars
JWT_EXPIRES_IN=1h  # Menor em testes

# CORS
CORS_ORIGINS=*